					<xsd:documentation xml:lang="en">Atomic number 2 - 'Helium', period 1, group 18, series 'Noble gas', mass 4 4.002602(2) g/mol</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Li">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Atomic number 3 - 'Lithium', period 2, group 1, series 'Alkali metal', mass 6 6.941(2) g/mol</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Be">
